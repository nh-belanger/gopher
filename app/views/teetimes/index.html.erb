<div class="row">
    <div class="homepage-title">
      <div class="small-1 large-6 small-centered large-centered columns">
          <div class="content-box-form-title">
            <h1 class="homepage-title small-3 large-6 small-centered large-centered">Current Teetimes</h1>
          </div>
      </div>
    </div>
</div>
<br>
<br>



  <div class="column text-center small-4 large-6 small-centered large-centered">
    <div class="teetimes-box">
<ul>
  <% @teetimes.each do |teetime| %>
    <li class = "teetime-list">
        <%= teetime.members.first.last_name %>, <%= teetime.members.first.first_name %>    |    <%= teetime.date.strftime("%m/%d/%Y") %> <%= teetime.time.strftime("%H:%M") %>
    </li>
  <% end %>
  </ul>
  </div>
</div>
